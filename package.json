{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "license": "UNLICENSED",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@cypress/code-coverage": "3.9.10",
    "@cypress/instrument-cra": "1.4.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.4",
    "@types/react-dom": "^17.0.9",
    "cypress": "8.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
    "start-server-and-test": "^1.13.1",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "@types/react": "^17.0.19",
    "@types/react-router-dom": "^5.1.8",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:start:app": "BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "cypress:start:wait": "start-server-and-test cypress:start:app http://localhost:3000",
    "cypress:run": "npm run cypress:start:wait -- \"cypress run\"",
    "e2e": "npm run cypress:start:wait -- \"cypress open\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
